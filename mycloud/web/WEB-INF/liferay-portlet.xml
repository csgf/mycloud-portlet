<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">
<liferay-portlet-app>
    <portlet>
        <portlet-name>myCloud4ChainREDS</portlet-name>
        <instanceable>false</instanceable>
        <header-portlet-css>/css/style.css</header-portlet-css>
        <header-portlet-css>/css/smoothness/jquery-ui.css</header-portlet-css>
        <header-portlet-javascript>/js/jquery-1.8.2.min.js</header-portlet-javascript>
        <header-portlet-javascript>/js/jquery-ui-1.9.1.custom.min.js</header-portlet-javascript>
        <header-portlet-javascript>/js/jquery.media.js</header-portlet-javascript>
           <header-portlet-javascript>/js/jquery.slimscroll.min.js</header-portlet-javascript>
    </portlet>
    <role-mapper>
        <role-name>administrator</role-name>
        <role-link>Administrator</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>guest</role-name>
        <role-link>Guest</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>power-user</role-name>
        <role-link>Power User</role-link>
    </role-mapper>
    <role-mapper>
        <role-name>user</role-name>
        <role-link>User</role-link>
    </role-mapper>
</liferay-portlet-app>
